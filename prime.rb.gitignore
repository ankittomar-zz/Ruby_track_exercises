def find_prime
	print ("Enter the End Limit")
	end_limit = gets.to_i
	h = 1
	while h<=end_limit
	  	prime = gen_prime(h)
	  	if prime == 0
	  		puts(h)
	  	end

	  	h+=1
	  end  
end

def gen_prime(num)
	h=2
	while h<num
		if num.remainder(h)==0
			return 1
		else
			h+=1
		end
	end
	return 0
end
	 
find_prime
